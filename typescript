[38;5;244m[48;5;233m 194 [m[38;5;253m[48;5;234m            current_state = [1m[38;5;142mtuple[m[38;5;253m[48;5;234m[[38;5;76m0[m[38;5;253m[48;5;234m]                                              [1;1H[38;5;244m[48;5;233m 165 [m[38;5;253m[48;5;234m[31;3H[38;5;39m[48;5;239m79[15;22H[?12l[?25h[?25l[m[38;5;253m[48;5;234m[1;1H                                                                                       
